<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:Uniation.ViewModels.Popups"
                    xmlns:popups1="clr-namespace:Uniation.Views.Popups" xmlns:popups11="clr-namespace:Uniation.Views.Pages">
    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:PaymentsMethodsViewModel}">
        <popups1:PaymentsMethodsView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:CardMethodPopupViewModel}">
        <popups1:CardMethodPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:QRMethodPopupViewModel}">
        <popups1:QRMethodPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:WaitingPopupViewModal}">
        <popups1:WaitingPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:SuccsessfulDonationViewModel}">
        <popups1:SuccsessfulDonationView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>