<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pages="clr-namespace:Uniation.ViewModels.Pages"
                    xmlns:pages1="clr-namespace:Uniation.Views.Pages">
    <DataTemplate DataType="{x:Type pages:MainPageViewModel}">
        <pages1:MainView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type pages:ProjectsPageViewModel}">
        <pages1:ProjectsView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type pages:ProjectCardViewModel}">
        <pages1:ProjectCartView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>