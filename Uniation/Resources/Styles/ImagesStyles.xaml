<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="Mainback">
        <Setter Property="Control.Height" Value="1080"/>
        <Setter Property="Control.Width" Value="1326"/>
        <Setter Property="Control.Margin" Value="0 0 0 0"/>
        <Setter Property="Control.HorizontalAlignment" Value="Left"/>
    </Style>

    <Style x:Key="Logo">
        <Setter Property="Control.Width" Value="370"/>
        <Setter Property="Control.Height" Value="120"/>
        <Setter Property="Control.Margin" Value="-1394,-846,0,0"/>
    </Style>

    <Style x:Key="CardImage">
        <Setter Property="Control.Width" Value="409"/>
        <Setter Property="Control.Height" Value="231"/>
        <Setter Property="Control.Margin" Value="0,24,0,0"/>
    </Style>

    <Style x:Key="Logo2" BasedOn="{StaticResource Logo}">
        <Setter Property="Control.Margin" Value="1354,-846,0,0"/>
    </Style>

    <Style x:Key="ProjInfoImage">
        <Setter Property="Control.Width" Value="823"/>
        <Setter Property="Control.Height" Value="463"/>
        <Setter Property="Control.Margin" Value="-836,-36,0,0"/>
    </Style>

    <Style x:Key="ProjBack1">
        <Setter Property="Control.Width" Value="703"/>
        <Setter Property="Control.Height" Value="652"/>
        <Setter Property="Control.HorizontalAlignment" Value="Left"/>
        <Setter Property="Control.VerticalAlignment" Value="Bottom"/>
    </Style>
    <Style x:Key="ProjBack2">
        <Setter Property="Control.Width" Value="658"/>
        <Setter Property="Control.Height" Value="554"/>
        <Setter Property="Control.HorizontalAlignment" Value="Right"/>
        <Setter Property="Control.Margin" Value="0,196,0,0"/>
    </Style>

    <Style x:Key="ProjCardBG">
        <Setter Property="Control.VerticalAlignment" Value="Bottom"/>
    </Style>
</ResourceDictionary>